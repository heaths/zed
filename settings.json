{
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4.5"
    },
    "model_parameters": []
  },
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "minimap": {
    "show": "auto"
  },
  "show_whitespaces": "boundary",
  "inlay_hints": {
    "enabled": true,
    "show_value_hints": true,
    "show_type_hints": true,
    "show_parameter_hints": true,
    "show_other_hints": true,
    "show_background": false,
    "edit_debounce_ms": 700,
    "scroll_debounce_ms": 50,
    "toggle_on_modifiers_press": {
      "control": false,
      "alt": false,
      "shift": false,
      "platform": false,
      "function": false
    }
  },
  "ui_font_size": 14,
  "buffer_font_size": 12,
  "buffer_line_height": "standard",
  "terminal": {
    "font_family": "Cascadia Code NF",
    "font_size": 12,
    "line_height": {
      "custom": 1.2
    }
  },
  "theme": {
    "mode": "system",
    "light": "Ayu Light",
    "dark": "Ayu Dark"
  },
  "theme_overrides": {
    "Ayu Light": {
      "syntax": {
        "hint": {
          "font_style": "italic"
        }
      }
    },
    "Ayu Dark": {
      "syntax": {
        "hint": {
          "font_style": "italic"
        }
      }
    }
  }
}
